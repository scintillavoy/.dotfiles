[user]
	email = scintillavoy@gmail.com
	name = Moon Dongyoung
[init]
	defaultBranch = main
[alias]
	a = add
	aa = add --all
	ai = add -i
	################
	b = branch
	ba = branch --all
	br = branch --remotes
	bd = branch --delete
	bD = branch --delete --force
	bm = branch --move
	bM = branch --move --force
	bnm = branch --no-merged
	bnma = branch --all --no-merged
	bnmr = branch --remotes --no-merged
	################
	bak = !git commit --message 'Backup commit' && git push --force-with-lease && git reset HEAD~ --soft
	################
	co = checkout
	cob = checkout -b
	coB = checkout -B
	col = checkout -
	################
	cf = config
	cfl = config --list
	cfll = config --list --local
	cfuser = !git config user.email 'scintillavoy@gmail.com' && git config user.name 'Moon Dongyoung'
	################
	cp = cherry-pick
	cpa = cherry-pick --abort
	cpc = cherry-pick --continue
	################
	ccc = !git reset --hard && git clean -df
	cccx = !git reset --hard && git clean -xdf
	cccb = !delete_merged_branches
	################
	cl = clone
	################
	c = commit
	ca = commit --all
	cm = commit --message
	cam = commit --all --message
	cd = commit --amend
	cda = commit --amend --all
	cdm = commit --amend --message
	cdam = commit --amend --all --message
	ce = commit --allow-empty
	cem = commit --allow-empty --message
	################
	f = fetch
	fp = fetch --prune
	################
	hfs = !hotfix_start
	hff = !hotfix_finish
	################
	i = init
	################
	l = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph
	la = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph --all
	lA = !git log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph $(git rev-list -g --all)
	lr = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --reverse
	# lt
	# lta
	# ltA
	# ltr
	lg = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph
	lga = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph --all
	lgA = !git log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --graph $(git rev-list -g --all)
	lgr = log --date=format:'%y-%m-%d %H:%M' --format='%C(auto)% D %s %Cgreen%cd %C(auto)%h %C(bold blue)%an' --reverse
	# lgt
	# lgta
	# lgtA
	# lgtr
	################
	m = merge
	ma = merge --abort
	mc = merge --continue
	mff = merge --ff-only
	mnff = merge --no-ff
	mnffnc = merge --no-ff --no-commit
	mdry = merge --no-ff --no-commit
	ms = merge --skip
	################
	pl = pull
	plff = pull --ff-only
	plnff = pull --no-ff
	################
	ps = push
	psoc = push origin HEAD
	psuoc = push -u origin HEAD
	psd = push --delete
	pst = push --tags
	################
	rb = rebase
	rba = rebase --abort
	rbc = rebase --continue
	rbd = rebase --reset-author-date
	rbdi = rebase --reset-author-date --interactive
	rbi = rebase --interactive
	rbs = rebase --skip
	################
	rls = !release_start
	rlf = !release_finish
	################
	rs = reset
	rss = reset --soft
	rsl = reset HEAD~
	rsls = reset HEAD~ --soft
	################
	rst = restore
	rsts = restore --staged
	################
	rmt = remote
	rmta = remote add
	rmtr = remote rm
	rmtv = remote -v
	rmtsu = remote set-url
	rmtgu = remote get-url
	################
	# root
	################
	rv = revert
	rvnc = revert --no-commit
	rvdry = revert --no-commit
	################
	s = status
	################
	st = stash
	sta = stash apply
	std = stash drop
	stl = stash list
	stp = stash pop
	stps = stash push --include-untracked
	stpss = stash push --staged
	sts = stash show
	################
	t = tag
	td = tag --delete
	tr = ls-remote --tags
